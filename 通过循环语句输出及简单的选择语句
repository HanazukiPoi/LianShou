#include <stdio.h>

int main() {
    char Y;
    int line = 0;
    int SB = 0;
    while (line < 30000) {
        printf("ciallo~: %d\n", line);
        line++;
    }
    if (line == 30000) {
        printf("hello\n");
    }
    printf("gzy帅不帅(Y/N)\n");
    scanf(" %c", &Y); 
    if (Y == 'Y' || Y == 'y') {
        printf("thx\n");
    } else if (Y == 'N'  || Y=='n') {
        while (SB < 1000000) {
            printf("铸币\n");
            SB++;
        }
        printf("NM\n");
    } else {

        printf("EMMMMM\n");
    }

    printf("thanks for your support!\n");
	return 0;
}
